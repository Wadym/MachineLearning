<?xml version="1.0" encoding="utf-8"?>
<MethodicConfig>
  <!--*****блока проверки (обработки) входных сигналов и определения установившихся режимов-->
  <InputsSettings>
    <!--время такта в системе, с-->
    <TimeStep>2</TimeStep>
    <!--длительность  установившегося режима, с-->
    <TimeSteady>1800</TimeSteady>
    <!--минимально допустимое время между установившимися режимами, с-->
    <TimeSteadyShift>60</TimeSteadyShift>
    <!--****настройка обработки параметров входных сигналов-->
    <!--Attribute = 0/1/2 (-1-используем значение по умолчагию;0 - не используем сигнал; 1 - используется ; 2- для некоторых сигналов дополнительная логика)-->
    <!-- Min/Max  - минимальное/максимальное допустимое значение сигнала-->
    <!-- ModeDetectorMin  - минимальное значение сигнала для Mode Detector-->
    <!--ModeDetectorDelta - максимальное отклонение на установившемся режиме-->
    <!-- если значение для всех не цифровое то параметер не рассматривается-->
    <Inputs>
      <Input ID="1">
        <!-- Температура окружающего воздуха, С-->
        <Attribute>2</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="2">
        <!-- Атмосферное давление, кПа-->
        <Attribute>1</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="3">
        <!-- Скорость вращения ротора низкого давления, об/мин-->
        <Attribute>2</Attribute>
        <Min>5</Min>
        <Max>*</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>1</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="4">
        <!-- Скорость вращения ротора высокого давления, об/мин-->
        <Attribute>1</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="5">
        <!-- Скорость вращения ротора нагнетателя, об/мин-->
        <Attribute>2</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="6">
        <!-- Температура газа на входе в нагнетатель, С-->
        <Attribute>1</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="7">
        <!-- Температура газа на выходе из нагнетателя, С-->
        <Attribute>0</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="8">
        <!-- Избыточное давление на входе нагнетателя, МПа-->
        <Attribute>0</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="9">
        <!-- Избыточное давление на выходе нагнетателя, МПа-->
        <Attribute>-1</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="10">
        <!-- Перепад давления на входном конфузоре ЦБН, кПа-->
        <Attribute>-1</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>20</DefaultValue>
      </Input>
      <Input ID="11">
        <!-- Температура на входе в осевой компрессор (t3). (температура на входе в ГТД), С-->
        <Attribute>-1</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="12">
        <!-- Расчетная температура за ГГ. (Температура за ТНД ГТД), С-->
        <Attribute>-2</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="13">
        <!-- Расход газа через ЦБН, взятый с САУ, кг/ч (м3/ч)-->
        <Attribute>-2</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>7</DefaultValue>
      </Input>
      <Input ID="14">
        <!-- Температура топливного газа, С-->
        <Attribute>-2</Attribute>
        <Min>5</Min>
        <Max>15</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>0</DefaultValue>
      </Input>
      <Input ID="15">
        <!-- Избыточное давление топливного газа, МПа-->
        <Attribute>0</Attribute>
        <Min>0</Min>
        <Max>1</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>0</DefaultValue>
      </Input>
      <Input ID="16">
        <!-- Перепад давления на диафрагме топливного газа, кПа-->
        <Attribute>0</Attribute>
        <Min>0</Min>
        <Max>1</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>0</DefaultValue>
      </Input>
      <Input ID="17">
        <!-- Расход топливного газа взятый с САУ, кг/ч(м3/ч)-->
        <Attribute>0</Attribute>
        <Min>0</Min>
        <Max>1</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>0</DefaultValue>
      </Input>
      <Input ID="18">
        <!-- P воздуха за ОК (P воздуха за КВД), МПа-->
        <Attribute>0</Attribute>
        <Min>0</Min>
        <Max>1</Max>
        <ModeDetectorMin>0</ModeDetectorMin>
        <ModeDetectorDelta>0</ModeDetectorDelta>
        <DefaultValue>0</DefaultValue>
      </Input>
      <Input ID="19">
        <!--плотность топливного газа, кг/м3-->
        <Attribute>0</Attribute>
        <Min>*</Min>
        <Max>200</Max>
        <ModeDetectorMin>25</ModeDetectorMin>
        <ModeDetectorDelta>*</ModeDetectorDelta>
        <DefaultValue>0</DefaultValue>
      </Input>
      <Input ID="20">
        <!--Теплотворная способность топливного газа, кДж/кг-->
        <Attribute>0</Attribute>
        <Min>10</Min>
        <Max>*</Max>
        <ModeDetectorMin>*</ModeDetectorMin>
        <ModeDetectorDelta>200</ModeDetectorDelta>
        <DefaultValue>0</DefaultValue>
      </Input>
    </Inputs>
    <!--***значения параметров по умолчанию-->
    <!--*данные параметры принимаються в случае отсутствия данных -->
    <!--*для указанных параметров на входе (eсли Attribute=0 в MethodicInputs)-->
  </InputsSettings>
  <!--*****блок настройки методики СТО-->
  <Parameters>
    <Polynoms>
      <!--задание коэффициентов полинома-->
      <!-- коэффициентов 1 полинома задаються в полях a00-a05-->
      <!--!!! коэффициентов полинома a10-a14 задаються только при задании двух полиномов на сечение-->
      <!--!!! в случае одного полинома ставяться точка раздела = "-", вторая часть игнорируется-->
      <!--Min/Max границы полинома  (за границами полином не имеет смысла и не расчитывается) -->
      <!--**Коэффициенты полинома СТО-->
      <Polynom ID="1">
        <!--*политропный КПД - расход  ЦБН, -->
        <num>1</num>
        <!--*количество полиномов на характеристику* -->
        <Min>1.0</Min>
        <Max>2.1</Max>
        <a00>1.1</a00>
        <a01>2</a01>
        <a02>3</a02>
        <a03>4</a03>
        <a04>5</a04>
        <a05>6</a05>
        <!--задание точки раздела полиномов-->
        <Point>10</Point>
        <!--задание коэффициентов 2 полинома-->
        <a10>7</a10>
        <a11>8</a11>
        <a12>9</a12>
        <a13>10</a13>
        <a14>11</a14>
        <a15>12</a15>
      </Polynom>
      <Polynom ID="2">
        <!--*Мощность, МВт- Температура за ТНД, С-->
        <num>2</num>
        <!--*количество полиномов на характеристику* -->
        <Min>2.0</Min>
        <Max>2.2</Max>
        <a00>10</a00>
        <a01>10</a01>
        <a02>10</a02>
        <a03>10</a03>
        <a04>10</a04>
        <a05>10</a05>
        <!--задание точки раздела полиномов-->
        <Point>0</Point>
        <!--задание коэффициентов 2 полинома-->
        <a10>0</a10>
        <a11>0</a11>
        <a12>0</a12>
        <a13>0</a13>
        <a14>0</a14>
        <a15>10</a15>
      </Polynom>
      <Polynom ID="3">
        <!--*Мощность, МВт- оборотыа КВД, С-->
        <num>3</num>
        <!--*количество полиномов на характеристику* -->
        <Min>3.1</Min>
        <Max>2.3</Max>
        <a00>10</a00>
        <a01>10</a01>
        <a02>10</a02>
        <a03>10</a03>
        <a04>10</a04>
        <a05>10</a05>
        <!--задание точки раздела полиномов-->
        <Point>0</Point>
        <!--задание коэффициентов 2 полинома-->
        <a10>0</a10>
        <a11>0</a11>
        <a12>0</a12>
        <a13>0</a13>
        <a14>0</a14>
        <a15>10</a15>
      </Polynom>
      <Polynom ID="4">
        <!--*Расход топлива, кг/ч- Температура за ТНД, С-->
        <num>4</num>
        <!--*количество полиномов на характеристику* -->
        <Min>4.1</Min>
        <Max>2.4</Max>
        <a00>0.5</a00>
        <a01>0.6</a01>
        <a02>0.7</a02>
        <a03>0.8</a03>
        <a04>0.9</a04>
        <a05>1.1</a05>
        <!--задание точки раздела полиномов-->
        <Point>9.5</Point>
        <!--задание коэффициентов 2 полинома-->
        <a10>7.5</a10>
        <a11>8.5</a11>
        <a12>9.5</a12>
        <a13>10.5</a13>
        <a14>11.5</a14>
        <a15>12.5</a15>
      </Polynom>
    </Polynoms>
    <!--свойства природного газа-->
    <!--параметры на номинальном режиме-->
    <Nominals>
      <Nominal ID="1">
        <!--Номинальная мощность машины, МВт -->
        <value>16.50</value>
      </Nominal>
      <Nominal ID="2">
        <!--Номинальные обороты вала низкого давления (двухвальная машина).К одновальным не применимо.-->
        <value>5200</value>
      </Nominal>
      <Nominal ID="3">
        <!--Номинальные обороты вала силовой турбины, об/мин-->
        <value>5800</value>
      </Nominal>
    </Nominals>
    <!--*****коэффициенты расхода шайб(конфузоров) мерных устройств -->
    <CoefficientDiafr>
      <Coeff ID="1">
        <!--Коэффициент расхода конфузора нагнетателя-->
        <value>43.95</value>
      </Coeff>
      <Coeff ID="2">
        <!--Коэффициент расхода топливной мерной шайбы-->
        <value>45.95</value>
      </Coeff>
    </CoefficientDiafr>
  </Parameters>
  <!--*****блок настройки (обработки - фильтрации) выходных сигналов-->
  <OutputsSettings>
    <Output ID="1">
      <!--КТС ЦБН-->
      <MedianFilter>1000</MedianFilter>
    </Output>
    <Output ID="2">
      <!--КТС ГТУ по мощности при ограничении по температуре-->
      <MedianFilter>2000</MedianFilter>
    </Output>
    <Output ID="3">
      <!--КТС ГТУ по мощности при ограничении по оборотам-->
      <MedianFilter>3000</MedianFilter>
    </Output>
    <Output ID="4">
      <!--КТС ГТУ по мощности при ограничении по расходу топливного газа-->
      <MedianFilter>4000</MedianFilter>
    </Output>
  </OutputsSettings>
</MethodicConfig>